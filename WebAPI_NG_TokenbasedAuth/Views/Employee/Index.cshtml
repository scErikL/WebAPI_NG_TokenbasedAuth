<div ng-app="app" ng-controller="emplcontroller">

    <h1>Employee Information</h1>
    <h2>{{Message}}</h2>
    <h2>
        <span>{{userName}}</span>
        <input type="button" class="btn" value="logoff" ng-click="logout()" />
    </h2>

    <table class="table table-striped table-striped table-bordered">
        <thead>
            <tr>
                <th>EmpNo</th>
                <th>EmpName</th>
                <th>Salary</th>
                <th>DeptName</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="Emp in Employees">
                <td>
                    <span>{{Emp.EmpNo}}</span>
                </td>
                <td>
                    <span>{{Emp.EmpName}}</span>
                </td>
                <td>
                    <span>{{Emp.Salary}}</span>
                </td>
                <td>
                    <span>{{Emp.DeptName}}</span>
                </td>
            </tr>
        </tbody>
    </table>
    <div><span>{{Message}}</span></div>
    <a href="http://~/Scripts/bootstrap.min.js" rel="nofollow" target="_blank">http://~/Scripts/bootstrap.min.js</a>
    <a href="http://~/Scripts/angular.min.js" rel="nofollow" target="_blank">http://~/Scripts/angular.min.js</a>
    <a href="http://~/App/Module.js" rel="nofollow" target="_blank">http://~/App/Module.js</a>

</div>
